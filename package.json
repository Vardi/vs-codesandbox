{
  "name": "vs-codesandbox",
  "displayName": "vs-codesandbox",
  "description": "Edit codesandbox in your vscode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codesandbox.login",
    "onCommand:codesandbox.openSandbox",
    "onCommand:codesandbox.previewSandbox",
    "onCommand:codesandbox.showInWebEditor",
    "onFileSystem:codesandboxfs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codesandbox.login",
        "title": "Login",
        "category": "Codesandbox",
        "detail": "Login to codesandbox",
        "requiresSignedOut": true
      },
      {
        "command": "codesandbox.openSandbox",
        "title": "Open sandbox",
        "category": "Codesandbox",
        "detail": "Open a sandbox",
        "promptsSignIn": true
      },
      {
        "command": "codesandbox.previewSandbox",
        "title": "Preview sandbox",
        "category": "Codesandbox",
        "detail": "Preview in browser"
      },
      {
        "command": "codesandbox.showInWebEditor",
        "title": "Show in web editor",
        "category": "Codesandbox",
        "detail": "Show sandbox in web editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.5",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "date-fns": "^2.6.0",
    "sander": "^0.6.0"
  }
}
